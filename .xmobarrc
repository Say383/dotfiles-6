Config
{ font = "-misc-fixed-medium-r-normal-*-13-*-*-*-*-*-*-*"
, additionalFonts = ["xft:Inconsolata Nerd Font-11"]
, bgColor = "#200000"
, fgColor = "#CFCFCF"
, position = Top
, overrideRedirect = False
, commands =
	[ Run Date "<fn=1>\xf5ef </fn>%a <action=`date +%F | xclip -r`>%F</action> %H:%M:%S" "theDate" 10
	, Run UnsafeXMonadLog
	, Run XPropertyLog "_XMOBAR_BATTERY_EXTRA"
	, Run Network "eth0" ["-L","0","-H","32768","--normal","green","--high","red","-t","<fn=1>\xf6ff </fn><rx>KB:<tx>KB "] 10
	, Run Network "wlan1" ["-L","0","-H","32768","--normal","green","--high","red","-t","<fn=1>\xfaa8 </fn><rx>KB|<tx>KB "] 10
	, Run BatteryN ["BAT0"] ["-L","5","-H","70","--high","green","--low","red","-d","1","-t","<acstatus><left>%|<watts>W","--","-O","<icon=power-ac.xbm/>","-i","<icon=power-ac.xbm/>","-o","<icon=power-bat.xbm/>"] 50 "battery0"
	, Run BatteryN ["BAT1"] ["-L","5","-H","70","--high","green","--low","red","-d","1","-t","|<left>%|<watts>W"] 50 "battery1"
	, Run CoreTemp ["-t","<icon=temp.xbm/><core0>C"] 50
	, Run MultiCpu ["-H","95","--high","red","-t","<icon=cpu.xbm/>(<autovbar>)","--","--contiguous-icons"] 10
	, Run Com "bash" ["-c", "[[ \"$(< /proc/modules)\" == *nvidia* ]] && echo \" <fc=red>nvidia</fc>\" || :"] "nvidia" 600
	, Run Com "bash" ["-c", "[[ \"$(/usr/local/sbin/liskin-gvt-g list)\" ]] && echo \" <fc=red>gvt</fc>\" || :"] "gvt" 600
	]
, sepChar = "%"
, alignSep = ""
, template = "%UnsafeXMonadLog% %gvt%%nvidia% %battery0%%battery1%%_XMOBAR_BATTERY_EXTRA% %coretemp% %multicpu% %eth0%%wlan1%<fc=white>%theDate%</fc>"
}
