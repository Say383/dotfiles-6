ACTION!="add", GOTO="liskin-autosuspend-end"

# T25 touchscreen
SUBSYSTEM=="usb", ENV{PRODUCT}=="2386/310e/0", GOTO="liskin-autosuspend-end"

# T25 card reader
SUBSYSTEM=="usb", ENV{PRODUCT}=="bda/316/204", GOTO="liskin-autosuspend-end"

# ThinkPad Ultra Dock
SUBSYSTEM=="usb", ENV{PRODUCT}=="17ef/1010/*", GOTO="liskin-autosuspend-end"

# usbhid devices (mouse, â€¦)
SUBSYSTEM=="usb", ENV{DRIVER}=="usbhid", GOTO="liskin-autosuspend-end"

TEST=="power/control", ATTR{power/control}="auto"
TEST=="power/autosuspend_delay_ms", ATTR{power/runtime_status}!="unsupported", ATTR{power/autosuspend_delay_ms}="2000"

LABEL="liskin-autosuspend-end"
